<h1>Add a Photo</h1>
<%= link_to "Back to Photos", photos_path %>
<!--Using photos_path instead or _url because we are in view and we don't need the full url, thats or controllers  -->

<%= form_for @photo do |f|%>
  <p>
    <%= f.label :artist %><br>
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>
